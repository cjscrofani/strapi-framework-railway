{
  "name": "@strapi-railway-framework/backend",
  "private": true,
  "version": "1.0.0",
  "description": "Strapi CMS backend with SendGrid integration",
  "keywords": [
    "strapi",
    "cms",
    "railway",
    "sendgrid"
  ],
  "scripts": {
    "build": "strapi build",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "migrate": "node scripts/migrate.js",
    "seed": "strapi develop --no-build --no-watch",
    "setup": "npm run migrate && npm run seed"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@mailchimp/mailchimp_transactional": "^1.0.59",
    "@railway/cli": "^4.6.3",
    "@sendgrid/client": "^8.1.5",
    "@sendgrid/mail": "^8.1.3",
    "@sentry/node": "^6.19.7",
    "@strapi/plugin-email": "^4.25.3",
    "@strapi/plugin-graphql": "^4.25.3",
    "@strapi/plugin-i18n": "^4.25.3",
    "@strapi/plugin-users-permissions": "^4.25.3",
    "@strapi/provider-email-sendgrid": "^4.25.3",
    "@strapi/strapi": "^4.25.3",
    "axios": "^1.6.8",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv-expand": "^12.0.2",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^6.2.0",
    "ioredis": "^5.7.0",
    "joi": "^18.0.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-schedule": "^2.1.1",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.6.1",
    "redis": "^4.6.13",
    "sharp": "^0.32.6",
    "winston": "^3.10.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=9.0.0"
  }
}
